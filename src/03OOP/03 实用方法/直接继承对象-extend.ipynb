    "var { displayRes } = require('utils')\n",
    "\n",
    "var extend = function(target, source) {\n",
    "  for (var prop in source) {\n",
    "    target[prop] = source[prop]\n",
    "  }\n",
    "\n",
    "  return target\n",
    "}\n",
    "\n",
    "var source = {\n",
    "  a: 1\n",
    "}\n",
    "var target = {\n",
    "  b: 2\n",
    "}\n",
    "extend(source, target)\n",
    "displayRes(source)"
   ]
  }
