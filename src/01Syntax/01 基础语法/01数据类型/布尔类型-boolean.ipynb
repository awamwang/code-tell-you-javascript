   ],
   "source": [
    "var {displayRes} = require('utils')\n",
    "\n",
    "var b = new Boolean(false);\n",
    "if (b) {\n",
    "  displayRes('b为真')\n",
    "}\n",
    "if (b == true) {\n",
    "  displayRes('b == true')\n",
    "} {\n",
    "  displayRes('b == true 为假')\n",
    "}\n",
    "displayRes(b === true)\n",
    "displayRes(b === false) // Boolean构造的对象与boolean类型字面量并不一样"
   ]
  }
