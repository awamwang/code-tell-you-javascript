   "source": [
    "var { displayRes } = require('utils')\n",
    "\n",
    "var o = {p: 42, q: true}\n",
    "\n",
    "// 用新变量名赋值\n",
    "var {p: foo, q: bar} = o;\n",
    "\n",
    "displayRes({foo, bar})"
   ]
  }
